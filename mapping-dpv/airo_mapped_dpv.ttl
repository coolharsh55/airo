terms:bibliographicCitation rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/contributor
terms:contributor rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/created
terms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
terms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
terms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/issued
terms:issued rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
terms:modified rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
terms:publisher rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/source
terms:source rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
terms:title rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/doi
<http://purl.org/ontology/bibo/doi> rdf:type owl:AnnotationProperty .


###  http://purl.org/ontology/bibo/status
<http://purl.org/ontology/bibo/status> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespacePrefix
<http://purl.org/vocab/vann/preferredNamespacePrefix> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


###  http://xmlns.com/foaf/0.1/logo
<http://xmlns.com/foaf/0.1/logo> rdf:type owl:AnnotationProperty .


#################################################################
#    Datatypes
#################################################################

###  http://www.w3.org/2001/XMLSchema#date
xsd:date rdf:type rdfs:Datatype .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/airo#compliesWithRegulation
airo:compliesWithRegulation rdf:type owl:ObjectProperty ;
                            rdfs:comment "Indicates compliance with a specific regulation."@en ;
                            rdfs:label "complies with regulation"@en ;
                            skos:note "Complies with regulation indicates evaluated status of being compliant. Instead presuming this property is about which regulation is associated for compliance" ;
                            skos:broader dpv:hasApplicableLaw .


###  https://w3id.org/airo#conformsToStandard
airo:conformsToStandard rdf:type owl:ObjectProperty ;
                        rdfs:range airo:Standard ;
                        rdfs:comment "Indicates conformance of an entity to a standard."@en ;
                        rdfs:label "conforms to standard"@en ;
                        skos:note "Conforms assumes that an evaluation has been performed and the implementation found conformant. This property can relate to several things: which standard is applicable or should be considered for conformance, and the status of conformance with that standard." ;
                        skos:exactMatch dpv:NotAvailable .


###  https://w3id.org/airo#detectsEvent
airo:detectsEvent rdf:type owl:ObjectProperty ;
                  rdfs:subPropertyOf airo:modifiesEvent ;
                  rdfs:domain airo:Control ;
                  rdfs:range airo:Event ;
                  rdfs:comment "Indicates the control used for detecting an event."@en ;
                  rdfs:label "detects event"@en ;
                  skos:note "DPV doesn't have a property to associate control with event. This should be added but without the event range as detection can also relate to threats, vulnerabilities, etc. So property name can be 'detects' and 'isDetectedBy'" ;
                  skos:exactMatch risk:DetectionControl .


###  https://w3id.org/airo#eliminatesEvent
airo:eliminatesEvent rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf airo:modifiesEvent ;
                     rdfs:domain airo:Control ;
                     rdfs:range airo:Event ;
                     rdfs:comment "Indicates the control used for eliminating an event."@en ;
                     rdfs:label "eliminates event"@en ;
                     skos:note "Eliminate refers to completely removing or nullifying the likelihood of an event. DPV doesn't have a property - this should be added as  'reducesLikelihoodOf' and 'eliminatesLikelihoodOf'" ;
                     skos:broader risk:ReduceLikelihood .


###  https://w3id.org/airo#exploitsVulnerability
airo:exploitsVulnerability rdf:type owl:ObjectProperty ;
                           rdfs:domain airo:RiskSource ;
                           rdfs:range airo:Vulnerability ;
                           rdfs:comment "Indicates vulnerabilities exploited by a risk source."@en ;
                           rdfs:label "exploits vulnerability"@en ;
                           skos:exactMatch risk:exploitsVulnerability .


###  https://w3id.org/airo#followsCodeOfConduct
airo:followsCodeOfConduct rdf:type owl:ObjectProperty ;
                          rdfs:comment "Indicates the code of conducts followed."@en ;
                          rdfs:label "follows code of conduct"@en ;
                          skos:note "DPV has a class for code of conduct associated using dpv:hasOrganisationalMeasure whereas this is a property." ;
                          skos:exactMatch dpv:CodeOfConduct .


###  https://w3id.org/airo#hasAISubject
airo:hasAISubject rdf:type owl:ObjectProperty ;
                  rdfs:domain airo:AISystem ;
                  rdfs:range airo:AISubject ;
                  rdfs:comment "Indicates the AI subject of an AI system."@en ;
                  rdfs:label "has AI subject"@en ;
                  skos:exactMatch tech:hasSubject .


###  https://w3id.org/airo#hasAIUser
airo:hasAIUser rdf:type owl:ObjectProperty ;
               rdfs:domain airo:AISystem ;
               rdfs:range airo:AIUser ;
               rdfs:comment "Indicate the end-user of an AI system."@en ;
               rdfs:label "has AI user"@en ;
               skos:exactMatch tech:hasUser .


###  https://w3id.org/airo#hasCapability
airo:hasCapability rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( airo:AIComponent
                                               airo:AISystem
                                             )
                               ] ;
                   rdfs:range airo:AICapability ;
                   rdfs:comment "Specifies capabilities implemented within an AI system to materialise its purposes."@en ;
                   rdfs:label "has capability"@en ;
                   skos:exactMatch tech:hasCapability ;
                   skos:note "hasCapability should be added to AI extension. Here capability is a property with domain as a union of two concepts. In DPV, the domain would be ai:AI" .


###  https://w3id.org/airo#hasComponent
airo:hasComponent rdf:type owl:ObjectProperty ;
                  rdfs:range airo:AIComponent ;
                  rdfs:comment "Indicates a component incorporating an AI system or another component."@en ;
                  rdfs:label "has component"@en ;
                  skos:exactMatch dpv:NotAvailable ;
                  skos:note "tech:Component exists as a class, but there is no property provided for associating it with tech:System. Propeties tech:hasComponent and tech:isComponentOf should be added." .


###  https://w3id.org/airo#hasConsequence
airo:hasConsequence rdf:type owl:ObjectProperty ;
                    rdfs:domain airo:Risk ;
                    rdfs:range airo:Consequence ;
                    rdfs:comment "Specifies consequences caused by materialisation of a risk."@en ;
                    rdfs:label "has consequence" ;
                    skos:exactMatch dpv:hasConsequence .


###  https://w3id.org/airo#hasControl
airo:hasControl rdf:type owl:ObjectProperty ;
                rdfs:range airo:Control ;
                rdfs:comment "Indicates the controls associated with a system or component."@en ;
                rdfs:label "has control"@en ;
                skos:exactMatch risk:hasControl .


###  https://w3id.org/airo#hasDocumentation
airo:hasDocumentation rdf:type owl:ObjectProperty ;
                      rdfs:range airo:Documentation ;
                      rdfs:comment "Indicates documentation associated with an entity."@en ;
                      rdfs:label "has documentation"@en ;
                      skos:exactMatch tech:hasDocumentation .


###  https://w3id.org/airo#hasExposure
airo:hasExposure rdf:type owl:ObjectProperty ;
                 rdfs:range airo:Exposure ;
                 rdfs:comment "Indicates exposure an entity to an event, e.g. exposure an affected entity to a specific impact."@en ; ## FIXME: Should be "Indicates exposure **of** an entity"
                 rdfs:label "has exposure"@en ;
                 skos:note "It is unclear what this is modelling." ;
                 skos:exactMatch dpv:NotApplicable .


###  https://w3id.org/airo#hasFrequency
airo:hasFrequency rdf:type owl:ObjectProperty ;
                  rdfs:domain airo:Event ;
                  rdfs:range airo:Frequency ;
                  rdfs:comment "Indicates frequency of an event."@en ;
                  rdfs:label "has frequency"@en ;
                  skos:exactMatch dpv:hasFrequency .


###  https://w3id.org/airo#hasImpact
airo:hasImpact rdf:type owl:ObjectProperty ;
               rdfs:domain airo:Consequence ;
               rdfs:range airo:Impact ;
               rdfs:comment "Specifies the impact caused by materialisation of a consequence."@en ;
               rdfs:label "has impact"@en ;
               skos:exactMatch dpv:hasImpact .


###  https://w3id.org/airo#hasImpactOnArea
airo:hasImpactOnArea rdf:type owl:ObjectProperty ;
                     rdfs:domain airo:Impact ;
                     rdfs:range airo:AreaOfImpact ;
                     rdfs:comment "Specifies the area (e.g. physical health) that is negatively impacted by an  AI system or component." ;
                     rdfs:label "has impact on area"@en ;
                     skos:note "This should probably be a property of the impact itself rather than an external assertion. E.g. Harm is the impact, and the domain/nature of that harm is on physical health. So this is either a subclass e.g. PhysicalHarm, or a property of that harm e.g. impactsArea Physical Health. I prefer the subclass mechanism as it support building a taxonomy of impacts" ;
                     skos:broader dpv:hasImpactOn .


###  https://w3id.org/airo#hasImpactOnEntity
airo:hasImpactOnEntity rdf:type owl:ObjectProperty ;
                       rdfs:domain airo:Impact ;
                       rdfs:range airo:Stakeholder ;
                       rdfs:comment "Indicates those that are negatively impacted by an AI system or component."@en ;
                       rdfs:label "has impact on entity"@en ;
                       skos:broader dpv:hasImpactOn .


###  https://w3id.org/airo#hasInput
airo:hasInput rdf:type owl:ObjectProperty ;
              rdfs:domain <http://w3id.org/airo#Entity> ; # FIXME: should be https://
              rdfs:range airo:Input ;
              rdfs:comment "Indicates the input an entity needs to process and/or generate output."@en ;
              rdfs:label "has input"@en ;
              skos:note "Not sure what this is modelling. It is data, then this should be dpv:hasData?" ;
              skos:exactMatch dpv:NotApplicable .


###  https://w3id.org/airo#hasLicense
airo:hasLicense rdf:type owl:ObjectProperty ;
                rdfs:range airo:License ;
                rdfs:comment "Indicates licenses associated with a resource."@en ;
                rdfs:label "has license"@en ;
                skos:note "DPV doesn't have a license property. Should be added to TECH as tech:License and tech:hasLicense" ;
                skos:exactMatch dpv:NotAvailable .


###  https://w3id.org/airo#hasLifecyclePhase
airo:hasLifecyclePhase rdf:type owl:ObjectProperty ;
                       rdfs:domain [ rdf:type owl:Class ;
                                     owl:unionOf ( airo:AIComponent
                                                   airo:AISystem
                                                 )
                                   ] ;
                       rdfs:range airo:AILifecyclePhase ;
                       rdfs:comment "Indicates the lifecycle phase of the AI system or component."@en ;
                       rdfs:label "has lifecycle phase"@en ;
                       skos:note "DPV doesn't have lifecycle of technology. Should be added to TECH as tech:LifecyclePhase and tech:hasLifecyclePhase" ;
                       skos:exactMatch dpv:NotAvailable .


###  https://w3id.org/airo#hasLikelihood
airo:hasLikelihood rdf:type owl:ObjectProperty ;
                   rdfs:domain airo:Event ;
                   rdfs:range airo:Likelihood ;
                   rdfs:comment "Indicates the probability of occurrence of an event."@en ;
                   rdfs:label "has likelihood"@en ;
                   skos:exactMatch dpv:hasLikelihood .


###  https://w3id.org/airo#hasModality
airo:hasModality rdf:type owl:ObjectProperty ;
                 rdfs:range airo:Modality ;
                 rdfs:comment "Indicates the modality in which an entity exists."@en ;
                 rdfs:label "has modality"@en ;
                 skos:note "Not sure what this is modelling" ;
                 skos:exactMatch dpv:NotApplicable .


###  https://w3id.org/airo#hasPreDeterminedChange
airo:hasPreDeterminedChange rdf:type owl:ObjectProperty ;
                            rdfs:range airo:Change ;
                            rdfs:comment "Indicates the changes that are planned to be applied to the system."@en ;
                            rdfs:label "has pre-determined change"@en ;
                            skos:note "Should be added to AI Act extension" ;
                            skos:exactMatch dpv:NotAvailable .


###  https://w3id.org/airo#hasPurpose
airo:hasPurpose rdf:type owl:ObjectProperty ;
                rdfs:range airo:Purpose ;
                rdfs:comment "Indicates the purpose of an entity."@en ;
                rdfs:label "has purpose"@en ;
                skos:exactMatch dpv:hasPurpose .


###  https://w3id.org/airo#hasQuality
airo:hasQuality rdf:type owl:ObjectProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( airo:AIComponent
                                            airo:AISystem
                                          )
                            ] ;
                rdfs:range airo:AIQuality ;
                rdfs:comment "Indicates a quality of an AI system or component."@en ;
                rdfs:label "has quality"@en ;
                skos:note "DQV should be used here e.g. dqv:hasQualityMeasurement? A note should be added to TECH to specify how to express quality, version, etc. using external properties" ;
                skos:exactMatch dpv:NotApplicable .

###  https://w3id.org/airo#hasRisk
airo:hasRisk rdf:type owl:ObjectProperty ;
             rdfs:range airo:Risk ;
             rdfs:comment "Indicates risks associated with an AI system, an AI component, etc."@en ;
             rdfs:label "has risk"@en ;
             skos:exactMatch dpv:hasRisk .


###  https://w3id.org/airo#hasSeverity
airo:hasSeverity rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( airo:Consequence
                                             airo:Impact
                                           )
                             ] ;
                 rdfs:range airo:Severity ;
                 rdfs:comment "Indicates severity of a consequence or an impact."@en ;
                 rdfs:label "has severity"@en ;
                 skos:exactMatch dpv:hasSeverity .


###  https://w3id.org/airo#hasStakeholder
airo:hasStakeholder rdf:type owl:ObjectProperty ;
                    rdfs:domain airo:AISystem ;
                    rdfs:range airo:Stakeholder ;
                    rdfs:comment "Indicates stakeholders of an AI system"@en ;
                    rdfs:label "has stakeholder"@en ;
                    skos:note "Stakeholder is too vague - DPV prefers explicit indication of entity roles." ;
                    skos:broader dpv:hasEntity .


###  https://w3id.org/airo#hasVersion
airo:hasVersion rdf:type owl:ObjectProperty ;
                rdfs:range airo:Version ;
                rdfs:comment "Indicates the version of an entity."@en ;
                rdfs:label "has version"@en ;
                skos:note "For versioning and other metadata, dct:hasVersion can be used, or DCAT should be used. It has e.g. dcat:version" ;
                skos:exactMatch dpv:NotApplicable .


###  https://w3id.org/airo#hasVulnerability
airo:hasVulnerability rdf:type owl:ObjectProperty ;
                      rdfs:range airo:Vulnerability ;
                      rdfs:comment "Indicates vulnerabilities associated with an entity, e.g. AI system and its components."@en ;
                      rdfs:label "has vulnerability"@en ;
                      skos:exactMatch risk:hasVulnerability .


###  https://w3id.org/airo#isAppliedWithinDomain
airo:isAppliedWithinDomain rdf:type owl:ObjectProperty ;
                           rdfs:domain airo:AISystem ;
                           rdfs:range airo:Domain ;
                           rdfs:comment "Specifies the domain an AI system is used within."@en ;
                           rdfs:label "is applied within domain"@en ;
                           skos:note "There is no definition for domain in EU law that I could find. Sector is defined, and there is NACE and NIS2 which specifically refer to sectors" ;
                           skos:broader dpv:hasSector .



###  https://w3id.org/airo#isDeployedBy
airo:isDeployedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( airo:AIComponent
                                              airo:AISystem
                                            )
                              ] ;
                  rdfs:range airo:AIDeployer ;
                  rdfs:comment "Indicates the deployer of an AI system or component."@en ;
                  rdfs:label "is deployed by"@en ;
                  skos:exactMatch tech:hasDeployer .


###  https://w3id.org/airo#isDevelopedBy
airo:isDevelopedBy rdf:type owl:ObjectProperty ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( airo:AIComponent
                                               airo:AISystem
                                             )
                               ] ;
                   rdfs:range airo:AIDeveloper ;
                   rdfs:comment "Indicates the developer of a system or component."@en ;
                   rdfs:label "is developed by"@en ;
                   skos:exactMatch tech:hasDeveloper .


###  https://w3id.org/airo#isFollowedByControl
airo:isFollowedByControl rdf:type owl:ObjectProperty ;
                         rdfs:domain airo:Control ;
                         rdfs:range airo:Control ;
                         rdfs:comment "Specifies the order of controls."@en ;
                         rdfs:label "is followed by control"@en ;
                         skos:note "DPV doesn't have control ordering, however dpv:isBefore and dpv:isAfter can be used here" ;
                         skos:broader dpv:isBefore .


###  https://w3id.org/airo#isPartOfControl
airo:isPartOfControl rdf:type owl:ObjectProperty ;
                     rdfs:domain airo:Control ;
                     rdfs:range airo:Control ;
                     rdfs:comment "Specifies composition of controls"@en ;
                     rdfs:label "is part of control"@en ;
                     skos:note "dct:hasPart should be used here" ;
                     skos:exactMatch dpv:NotApplicable .


###  https://w3id.org/airo#isProvidedBy
airo:isProvidedBy rdf:type owl:ObjectProperty ;
                  rdfs:domain [ rdf:type owl:Class ;
                                owl:unionOf ( airo:AIComponent
                                              airo:AISystem
                                            )
                              ] ;
                  rdfs:range airo:AIProvider ;
                  rdfs:comment "Indicates the provider of an AI system or component."@en ;
                  rdfs:label "is provided by"@en ;
                  skos:exactMatch tech:hasProvider .


###  https://w3id.org/airo#isRiskSourceFor
airo:isRiskSourceFor rdf:type owl:ObjectProperty ;
                     rdfs:domain airo:RiskSource ;
                     rdfs:range airo:Risk ;
                     rdfs:comment "Specifies risks caused by materialisation of a risk source."@en ;
                     rdfs:label "is risk source for"@en ;
                     skos:exactMatch dpv:hasRisk .


###  https://w3id.org/airo#isUsedWithinEnvironment
airo:isUsedWithinEnvironment rdf:type owl:ObjectProperty ;
                             rdfs:domain [ rdf:type owl:Class ;
                                           owl:unionOf ( airo:AIComponent
                                                         airo:AISystem
                                                       )
                                         ] ;
                             rdfs:range airo:EnvironmentOfUse ;
                             rdfs:comment "Indicates the environment of use."@en ;
                             rdfs:label "is used within environment"@en ;
                             skos:note "Unclear on what environment means here, whether it includes conditions, operation, people involved, public/private location, weather conditions, etc. Specific properties already exist for location, users, etc." ;
                             skos:exactMatch dpv:NotApplicable .


###  https://w3id.org/airo#mitigatesEvent
airo:mitigatesEvent rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf airo:modifiesEvent ;
                    rdfs:domain airo:Control ;
                    rdfs:range airo:Event ;
                    rdfs:comment "Indicates the control used for mitigating an event."@en ;
                    rdfs:label "mitigates event"@en ;
                    skos:note "DPV doesn't contain properties for indicating mitigation or modification of events. Properties should be added as: risk:modifies and risk:isModifiedBy, risk:mitigates and risk:isMitigatedBy" ;
                    skos:broader risk:ReduceSeverity .


###  https://w3id.org/airo#modifiesEvent
airo:modifiesEvent rdf:type owl:ObjectProperty ;
                   rdfs:domain airo:Control ;
                   rdfs:range airo:Event ;
                   rdfs:comment "Indicates the control employed to modify an event."@en ;
                   rdfs:label "modifies event"@en ;
                   skos:broader risk:RiskControl .



###  https://w3id.org/airo#producesOutput
airo:producesOutput rdf:type owl:ObjectProperty ;
                    rdfs:domain <http://w3id.org/airo#Entity> ; ## FIXME: should be https://
                    rdfs:range airo:Output ;
                    rdfs:comment "Indicate the output generated by an entity."@en ;
                    rdfs:label "produces output"@en ;
                    skos:note "Not sure what this is referring to. If this is the inverse of tech:hasProducer i.e. tech:isProducedBy then that's clear." ;
                    skos:exactMatch dpv:NotApplicable .


###  https://w3id.org/airo#providesInput
airo:providesInput rdf:type owl:ObjectProperty ;
                   rdfs:comment "Indicates an entity that provides inputs to another entity."@en ;
                   rdfs:label "provides input"@en ;
                   skos:note "DPV doesn't have input and output. It does have Data Source and Recipient" ;
                   skos:broader dpv:hasDataSource .



###  https://w3id.org/airo#usesTechnique
airo:usesTechnique rdf:type owl:ObjectProperty ;
                   rdfs:domain airo:AISystem ;
                   rdfs:range airo:AITechnique ;
                   rdfs:comment "Indicates the AI techniques used in an AI system or component."@en ;
                   rdfs:label "uses technique"@en ;
                   skos:note "AI extension has Technique but no property. Should be added as ai:hasTechnique" ;
                   skos:broader dpv:NotAvailable .


#################################################################
#    Data properties
#################################################################

###  http://purl.org/dc/terms/date
terms:date rdf:type owl:DatatypeProperty .


#################################################################
#    Classes
#################################################################

###  http://spdx.org/rdf/terms#License
<http://spdx.org/rdf/terms#License> rdf:type owl:Class ;
                                    owl:equivalentClass airo:License .


###  http://w3id.org/airo#Entity
<http://w3id.org/airo#Entity> rdf:type owl:Class ; ## FIXME: should be https://
                              terms:source "ISO/IEC TR 24028, 3.16"@en ;
                              rdfs:comment "Any concrete or abstract thing of interest."@en ;
                              rdfs:label "Entity"@en .


###  https://w3id.org/airo#AICapability
airo:AICapability rdf:type owl:Class ;
                  rdfs:comment "The capability of an AI system that enables realisation of the system's purposes."@en ;
                  rdfs:label "AI Capability"@en ;
                  skos:exactMatch ai:Capability .


###  https://w3id.org/airo#AIComponent
airo:AIComponent rdf:type owl:Class ;
                 rdfs:subClassOf <http://w3id.org/airo#Entity> ;
